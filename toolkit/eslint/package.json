{
  "name": "@toolkit/eslint",
  "version": "1.0.0",
  "type": "module",
  "exports": {
    ".": {
      "default": "./index.js",
      "import": "./index.js",
      "require": "./index.cjs",
      "types": "./index.d.ts"
    }
  },
  "main": "./index.cjs",
  "module": "./index.js",
  "types": "./index.d.ts",
  "bin": {
    "eslint-demo": "./index.js"
  },
  "scripts": {
    "lint": "eslint --fix",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --verbose --timeout=1000",
    "dev": "tsx ./index.js",
    "build": "tsc --project tsconfig.build.json && tsc-alias --project tsconfig.build.json",
    "build:watch": "tsc --project tsconfig.build.json && (concurrently \"tsc --project tsconfig.build.json -w\" \"tsc-alias --project tsconfig.build.json -w\")",
    "start": "node index.js"
  },
  "dependencies": {
    "react": "^19.1.1",
    "tsx": "^4.20.5"
  },
  "devDependencies": {
    "@jest/globals": "^30.1.2",
    "@stylistic/eslint-plugin": "^5.3.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.0",
    "eslint": "^9.35.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^16.4.0",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "vue-eslint-parser": "^10.2.0"
  }
}
