{
  "name": "@toolkit/eslint",
  "version": "1.0.0",
  "type": "module",
  "exports": {
    ".": {
      "default": "./index.js",
      "import": "./index.js",
      "require": "./index.cjs",
      "types": "./index.d.ts"
    }
  },
  "main": "./index.cjs",
  "module": "./index.js",
  "types": "./index.d.ts",
  "bin": {
    "eslint-demo": "./index.js"
  },
  "scripts": {
    "lint": "eslint --fix",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --verbose --timeout=1000",
    "dev": "tsx ./index.js",
    "build": "tsc --project tsconfig.build.json && tsc-alias --project tsconfig.build.json",
    "build:watch": "tsc --project tsconfig.build.json && (concurrently \"tsc --project tsconfig.build.json -w\" \"tsc-alias --project tsconfig.build.json -w\")",
    "start": "node index.js"
  },
  "dependencies": {
    "tsx": "^4.19.3"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-vue": "^9.33.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "tsc-alias": "^1.8.15",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "vue-eslint-parser": "^10.1.3"
  }
}
