{
  "name": "babel-lib",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js"
    }
  },
  "main": "./index.js",
  "module": "./index.mjs",
  "scripts": {
    "lint": "eslint src/**/*.{js,mjs,cjs} --fix",
    "test": "mocha src/**/*.spec.{js,mjs,cjs} --require @babel/register",
    "build": "babel src --out-dir dist --config-file ./babel.build.config.js",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/node": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/register": "^7.25.9",
    "babel-plugin-add-import-extension": "^1.6.0",
    "chai": "4.4.1",
    "eslint": "^9.16.0",
    "globals": "^15.13.0",
    "mocha": "^11.0"
  }
}
