    "babelify": "^7.3.0",
    "bootstrap": "^3.3.7",
    "browserify": "^13.1.1",
    "browserify-shim": "^3.8.12",
    "bundle-collapser": "^1.2.1",
    "glob": "^7.1.1",
    "gulp": "^3.9.1",
    "gulp-base64": "^0.1.3",
    "gulp-clean": "^0.3.2",
    "gulp-clean-css": "^2.0.13",
