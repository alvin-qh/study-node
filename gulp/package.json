{
    "name": "gulp.demo",
    "version": "0.0.1",
    "description": "An gulp demo project",
    "scripts": {
        "build": "node ./node_modules/.bin/babel-node ./node_modules/.bin/gulp",
        "start": "node ./node_modules/.bin/babel-node ./www/bin/www",
        "dev": "node ./node_modules/.bin/nodemon",
        "setup": "npm install --update-binary"
    },
    "browser": {
        "jquery": "./node_modules/jquery/dist/jquery.js",
        "bootstrap-notify": "./node_modules/bootstrap-notify/bootstrap-notify.js"
    },
    "browserify": {
        "transform": [
            "browserify-shim"
        ]
    },
    "browserify-shim": {
        "jquery": {
            "exports": "global:$"
        },
        "bootstrap-notify": {
            "depends": [
                "jquery"
            ]
        }
    },
    "dependencies": {
        "babel-cli": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "babel-runtime": "^6.26.0",
        "body-parser": "^1.15.2",
        "cookie-parser": "^1.4.3",
        "debug": "^3.1.0",
        "express": "^4.14.0",
        "express-validator": "^5.0.3",
        "morgan": "^1.7.0",
        "nunjucks": "^3.1.2"
    },
    "devDependencies": {
        "babel-core": "^6.26.0",
        "babel-preset-es2015": "^6.18.0",
        "babelify": "^8.0.0",
        "bootstrap": "^4.0.0",
        "browserify": "^16.1.1",
        "browserify-shim": "^3.8.12",
        "bundle-collapser": "^1.2.1",
        "glob": "^7.1.1",
        "gulp": "^3.9.1",
        "gulp-base64": "^0.1.3",
        "gulp-clean": "^0.4.0",
        "gulp-clean-css": "^3.9.3",
        "gulp-concat": "^2.6.0",
        "gulp-if-else": "^1.0.3",
        "gulp-minify-html": "^1.0.6",
        "gulp-notify": "^3.2.0",
        "gulp-rev": "^8.1.1",
        "gulp-rev-collector": "^1.0.5",
        "gulp-sourcemaps": "^2.2.0",
        "gulp-uglify": "^3.0.0",
        "gulp-util": "^3.0.8",
        "gulp-watch": "^5.0.0",
        "jquery": "^3.1.1",
        "merge-stream": "^1.0.1",
        "nodemon": "^1.17.2",
        "parsleyjs": "^2.5.1",
        "run-sequence": "^2.2.1",
        "stripify": "^6.0.0",
        "vinyl-buffer": "^1.0.0",
        "vinyl-source-stream": "^2.0.0",
        "watchify": "^3.7.0"
    },
    "babel": {
        "presets": [
            "env"
        ],
        "plugins": [
            "transform-runtime"
        ]
    }
}
