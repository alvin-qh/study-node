{
  "name": "@orm/prisma",
  "version": "1.0.0",
  "description": "Study orm libs",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint --fix",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --verbose --cache",
    "generate": "prisma generate",
    "migrate": "prisma migrate dev",
    "build": "tsc --project tsconfig.build.json && tsc-alias --project tsconfig.build.json",
    "build:watch": "tsc --project tsconfig.build.json && (concurrently \"tsc --project tsconfig.build.json -w\" \"tsc-alias --project tsconfig.build.json -w\")"
  },
  "dependencies": {
    "@prisma/client": "6.16.2",
    "dayjs": "^1.11.18",
    "dotenv": "^17.2.2",
    "lodash": ">=4.17.21",
    "mysql2": "^3.15.0",
    "prisma": "^6.16.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@jest/globals": "^30.1.2",
    "@stylistic/eslint-plugin": "^5.4.0",
    "eslint": "^9.36.0",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^16.4.0",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.4",
    "tsc-alias": "^1.8.16",
    "typescript-eslint": "^8.44.1"
  },
  "optionalDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2"
  }
}
