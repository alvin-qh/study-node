{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules"
  ],
  "verbose": true,
  "execMap": {
    "": "./node_modules/.bin/babel-node"
  },
  "events": {
    "restart": "",
    "start": "",
    "crash": "",
    "exit": ""
  },
  "watch": [
    "src/app/**/*.js"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json"
}
