{
  "name": "@web/bun",
  "version": "1.0.0",
  "type": "module",
  "main": "index.ts",
  "module": "index.ts",
  "scripts": {
    "lint": "eslint --fix",
    "start": "bun ./index.ts",
    "build": "tsc --project tsconfig.build.json && tsc-alias --project tsconfig.build.json",
    "build:watch": "tsc --project tsconfig.build.json && (concurrently \"tsc --project tsconfig.build.json -w\" \"tsc-alias --project tsconfig.build.json -w\")"
  },
  "dependencies": {
    "url-pattern": "^1.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@stylistic/eslint-plugin": "^5.3.1",
    "@types/bun": "latest",
    "@types/node": "^24.5.2",
    "eslint": "^9.35.0",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^16.4.0",
    "typescript-eslint": "^8.44.0"
  },
  "optionalDependencies": {
    "typescript": "^5.9.2",
    "tsc-alias": "^1.8.16"
  }
}
