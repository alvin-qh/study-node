{
  "name": "@web/axios",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint --fix",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --verbose --detectOpenHandles",
