  "dependencies": {
    "@koa/router": "^14.0.0",
    "@ladjs/koa-views": "^9.0.0",
    "axios": "^1.12.2",
    "dotenv": "^17.2.2",
    "form-data": "^4.0.4",
    "fs-extra": "^11.3.2",
    "koa": "^3.0.1",
    "koa-body": "^6.0.1",
    "nunjucks": "^3.2.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@jest/globals": "^30.1.2",
    "@stylistic/eslint-plugin": "^5.4.0",
    "cheerio": "^1.1.2",
    "eslint": "^9.36.0",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^16.4.0",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.4",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0"
  },
  "optionalDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^30.0.0",
    "@types/koa": "*",
    "@types/koa__router": "^12.0.4",
    "@types/node": "^24.5.2",
    "@types/nunjucks": "^3.2.6"
