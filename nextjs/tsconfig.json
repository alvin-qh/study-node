{
  "compilerOptions": {
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "lib": [
      "ESNext"
    ],
    "module": "ESNext",
    "moduleResolution": "Node",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "outDir": "dist",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "removeComments": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "sourceRoot": ".",
    "strict": true,
    "target": "ESNext",
    "types": [
      "node"
    ],
    "useUnknownInCatchVariables": false,
    "allowJs": true,
    "noEmit": true,
    "incremental": true,
    "jsx": "preserve"
  },
  "include": [
    "pages/**/*.d.ts",
    "pages/**/*.spec.ts",
    "pages/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
