{
  "all": true,
  "include": [
    "src/**/*.spec.ts"
  ],
  "exclude": [
    ".coverage",
    ".history",
    "dist",
    "node_modules"
  ],
  "reporter": [
    "text",
    "html",
    "icov"
  ],
  "report-dir": "./.coverage",
  "excludeNodeModules": true,
  "watermarks": {
    "lines": [
      80,
      95
    ],
    "functions": [
      80,
      95
    ],
    "branches": [
      80,
      95
    ],
    "statements": [
      80,
      95
    ]
  }
}
