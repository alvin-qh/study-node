    "@jest/globals": "^30.1.2",
    "@stylistic/eslint-plugin": "^5.3.1",
    "@types/jest": "^30.0.0",
    "eslint": "^9.35.0",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^16.4.0",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.1",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0"
  }
